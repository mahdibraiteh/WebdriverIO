            steps {
                echo 'building'
            }}
            stage('deploy') {
            steps {
                echo 'Deploying'
            }}
            stage('test') {
            steps {
                echo 'testting'
            }}
            stage('Release') {
            steps {
                echo 'Releasing'
            }}
        }
    }
